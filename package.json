    "reactship": "./bin/index.js"
  },
  "dependencies": {
    "chalk": "1.1.3"
  },
  "devDependencies": {
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "node-fetch": "1.7.3"
  },
  "upperDependencies": {
    "react-dom": "16.2.0",
    "react-native": "0.51.0"
  }
}
